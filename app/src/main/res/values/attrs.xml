<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="needSelected" format="boolean"/>
    <attr name="needApplyTextColor" format="boolean"/>
    <attr name="needApplyDrawableColor" format="boolean"/>
    <attr name="normalDrawableColor" format="reference|color"/>



    <declare-styleable name="CMRoundedCorner">
        <attr name="cmRoundedCorner" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomThemeTextView">
        <attr name="needSelected" />
        <attr name="needApplyTextColor" />
        <attr name="needApplyDrawableColor" />
        <attr name="normalDrawableColor" />
    </declare-styleable>

    <declare-styleable name="DrawableCenterTextView">
        <attr name="needRightHint" format="boolean"/>
        <attr name="hintLeftMargin" format="dimension"/>
        <attr name="hintTextSize" format="dimension"/>
        <attr name="hintRadius" format="dimension"/>
        <attr name="hintText" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomThemeTabLayout">
        <attr name="tabType">
            <enum name=""
        </attr>
    </declare-styleable>
</resources>